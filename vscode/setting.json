// 既定の設定を上書きするには、このファイル内に設定を挿入します
{
	"terminal.external.osxExec": "Hyper.app",
	"materialTheme.fixIconsRunning": false,
	"workbench.startupEditor": "newUntitledFile",
	"terminal.integrated.rendererType": "dom",
	// "terminal.integrated.shell.osx": "/usr/local/bin/fish",
	"terminal.integrated.defaultProfile.osx": "fish",
	"terminal.integrated.fontFamily": "Droid Sans Mono for Powerline, Menlo,DejaVu Sans Mono,Lucida Console,monospace",
	"javascript.updateImportsOnFileMove.enabled": "always",
	"editor.tabSize": 2,
	"eslint.packageManager": "yarn",
	"npm.packageManager": "yarn",
	"indentRainbow.indentSetter": {
		"javascript": { "tabSize": 2, "insertSpaces": true }
	},
	"workbench.iconTheme": "material-icon-theme",
	"swaggerViewer.defaultPort": 9001,
	"editor.wordWrap": "off",
	"typescript.updateImportsOnFileMove.enabled": "always",
	"vsicons.dontShowNewVersionMessage": true,
	"editor.fontSize": 16,
	"editor.suggestSelection": "first",
	"vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
	"workbench.colorTheme": "Material Theme Darker",
	"stylelint.autoFixOnSave": true,
	"git.ignoreMissingGitWarning": true,
	"yaml.schemaStore.enable": false,
	"editor.codeActionsOnSave": {},
	"editor.formatOnSave": true,
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"prettier.jsxSingleQuote": true,
	"prettier.singleQuote": true,
	"settingsSync.ignoredSettings": ["ruby.rubocop.configFilePath"],
	"editor.cursorStyle": "line",
	"editor.lineNumbers": "on",
	"editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
	"vscode-erb-beautify.tab": true,
	"prettier.useTabs": true,
	"yaml.schemas": {},
	"yaml.customTags": [
		"!And",
		"!And sequence",
		"!If",
		"!If sequence",
		"!Not",
		"!Not sequence",
		"!Equals",
		"!Equals sequence",
		"!Or",
		"!Or sequence",
		"!FindInMap",
		"!FindInMap sequence",
		"!Base64",
		"!Join",
		"!Join sequence",
		"!Cidr",
		"!Ref",
		"!Sub",
		"!Sub sequence",
		"!GetAtt",
		"!GetAZs",
		"!ImportValue",
		"!ImportValue sequence",
		"!Select",
		"!Select sequence",
		"!Split",
		"!Split sequence"
	],
	"window.zoomLevel": 2,
	"terminal.integrated.automationShell.osx": "",
	"ruby.rubocop.executePath": "/Users/inomar14/.anyenv/envs/rbenv/shims/",
	"redhat.telemetry.enabled": true,
	"json.schemas": []
}
