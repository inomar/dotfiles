// 既定の設定を上書きするには、このファイル内に設定を挿入します
{
  "terminal.external.osxExec": "Hyper.app",
  "materialTheme.fixIconsRunning": false,
  "workbench.startupEditor": "newUntitledFile",
  "terminal.integrated.rendererType": "dom",
  "terminal.integrated.profiles.osx": {
    "fish": {
      "path": "/usr/local/bin/fish",
      "args": ["-l"]
    }
  },
  "terminal.integrated.defaultProfile.osx": "fish",
  "terminal.integrated.fontFamily": "Droid Sans Mono for Powerline, Menlo,DejaVu Sans Mono,Lucida Console,monospace",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "editor.tabSize": 2,
  "eslint.packageManager": "yarn",
  "npm.packageManager": "yarn",
  "indentRainbow.indentSetter": {
    "javascript": { "tabSize": 2, "insertSpaces": true }
  },
  "workbench.iconTheme": "material-icon-theme",
  "swaggerViewer.defaultPort": 9001,
  "editor.wordWrap": "off",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "vsicons.dontShowNewVersionMessage": true,
  "editor.fontSize": 16,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "stylelint.autoFixOnSave": true,
  "git.ignoreMissingGitWarning": true,
  "yaml.schemaStore.enable": false,
  "editor.codeActionsOnSave": {},
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "prettier.jsxSingleQuote": true,
  "prettier.singleQuote": true,
  "editor.cursorStyle": "line",
  "editor.lineNumbers": "on",
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  "vscode-erb-beautify.tab": true,
  "yaml.schemas": {},
  "yaml.customTags": [
    "!And",
    "!And sequence",
    "!If",
    "!If sequence",
    "!Not",
    "!Not sequence",
    "!Equals",
    "!Equals sequence",
    "!Or",
    "!Or sequence",
    "!FindInMap",
    "!FindInMap sequence",
    "!Base64",
    "!Join",
    "!Join sequence",
    "!Cidr",
    "!Ref",
    "!Sub",
    "!Sub sequence",
    "!GetAtt",
    "!GetAZs",
    "!ImportValue",
    "!ImportValue sequence",
    "!Select",
    "!Select sequence",
    "!Split",
    "!Split sequence"
  ],
  "ruby.lint": {
    "rubocop": true,
    "reek": true
  },
  "ruby.format": "rubocop",
  "redhat.telemetry.enabled": true,
  "json.schemas": [],
  "git.autofetch": true,
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "audioCues.lineHasError": "off",
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": "active",
  "editor.accessibilitySupport": "off",
  "aws.suppressPrompts": {
    "regionAddAutomatically": true
  },
  "diffEditor.ignoreTrimWhitespace": false,
  "workbench.colorTheme": "Material Theme Darker High Contrast",
  "editor.suggest.showMethods": true,
  "editor.suggest.preview": true,
  "editor.acceptSuggestionOnEnter": "on",
  "editor.snippetSuggestions": "top",
  "window.zoomLevel": 2,
  "npm.keybindingsChangedWarningShown": true,
  "markdown.extension.preview.autoShowPreviewToSide": true,
  "prettier.proseWrap": "always",
  "settingsSync.ignoredSettings": ["ruby.rubocop.configFilePath"],
  "editor.renderWhitespace": "boundary",
  "editor.formatOnSave": true,
  "javascript.format.insertSpaceAfterCommaDelimiter": false,
  "editor.inlineSuggest.enabled": true,
  "github.copilot.enable": {
    "*": true,
    "yaml": true,
    "plaintext": false,
    "markdown": false,
    "scminput": false
  },
  "github.copilot.advanced": {}
}
